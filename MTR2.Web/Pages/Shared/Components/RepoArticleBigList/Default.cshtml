@model MTR2.Web.ViewComponents.RepoArticleBigListViewComponent.RepoArticleListModel

<nav id="ref-articles" class="mt-3">
	<ul>
		@foreach (var repoArticle in Model.RepoArticles)
		{
		<li>
			<a asp-page="RArticle" asp-route-id="@repoArticle.Id">@repoArticle.Order @repoArticle.Title</a>
		</li>
		}
	</ul>
</nav>